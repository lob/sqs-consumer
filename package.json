{
  "name": "sqs-consumer",
  "version": "0.1.0",
  "description": "Build SQS-based Node applications without the boilerplate",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc",
    "watch": "tsc --watch",
    "clean": "rm -fr dist/*",
    "prepublish": "npm run build",
    "pretest": "npm run build",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "lint": "eslint . --ext .ts --fix",
    "lint:fix": "eslint . --fix",
    "coverage": "nyc mocha && nyc report --reporter=html && nyc report --reporter=json-summary",
    "lcov": "nyc mocha && nyc report --reporter=lcov",
    "posttest": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lob/sqs-consumer.git"
  },
  "bugs": {
    "url": "https://github.com/lob/sqs-consumer/issues"
  },
  "homepage": "https://github.com/lob/sqs-consumer",
  "keywords": [
    "sqs",
    "queue",
    "consumer"
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/jest": "^29.1.2",
    "@types/node": "^18.8.4",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "aws-sdk": "^2.1114.0",
    "aws-sdk-client-mock": "^2.0.0",
    "aws-sdk-client-mock-jest": "^2.0.0",
    "codeclimate-test-reporter": "^0.5.1",
    "eslint": "^8.25.0",
    "eslint-config-iplayer-ts": "^4.1.0",
    "generate-changelog": "latest",
    "jest": "^29.1.2",
    "nyc": "^15.1.0",
    "p-event": "^5.0.1",
    "source-map": "^0.6.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "debug": "^4.3.4"
  },
  "peerDependencies": {
    "@aws-sdk/client-sqs": "^3.186.0"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "eslintConfig": {
    "extends": "iplayer-ts",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    }
  },
  "mocha": {
    "spec": "test/**/**/*.ts",
    "require": "ts-node/register"
  }
}
